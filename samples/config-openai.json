{
  "llm_models": [
    {
      "name": "gpt-3.5-turbo",
      "api_key": "YOUR_OPENAI_API_KEY",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "max_tokens": 4096,
      "temperature": 0.7,
      "timeout_seconds": 30
    }
  ],
  "output_settings": {
    "output_directory": "./docs",
    "format": "markdown",
    "include_icons": true,
    "generate_unit_tests": true,
    "target_coverage": 0.9,
    "generate_mermaid": true,
    "mermaid_output_path": "./diagrams",
    "mermaid_naming": {
      "prefix": "",
      "suffix": "",
      "extension": "mmd"
    },
    "generate_plantuml": true,
    "plantuml_output_path": "./diagrams",
    "plantuml_naming": {
      "prefix": "",
      "suffix": "",
      "extension": "puml"
    },
    "error_log": "errors.log",
    "output_log": "output.log",
    "verbose_output": false,
    "overwrite": false,
    "collision_strategy": "timestamp",
    "backup_existing": true
  },
  "analysis_settings": {
    "include_private_members": false,
    "max_threads": 4,
    "supported_languages": [
      "java",
      "python"
    ],
    "exclude_patterns": [
      "**/test/**",
      "**/target/**",
      "**/build/**"
    ]
  }
}
